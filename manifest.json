{
    "manifest_version": 3,
    "name": "LeetCode Friends List",
    "version": "1.0",
    "description": "Adds a friends list to LeetCode profile pages",
    "permissions": ["storage"],
    "host_permissions": ["https://leetcode.com/*"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/u/*/"],
        "js": ["content.js"]
      }
    ]
  }